fn CFAbsoluteTimeGetDifferenceAsGregorianUnits( CFAbsoluteTime at1, CFAbsoluteTime at2, CFTimeZoneRef tz, CFOptionFlags unitFlags ) = CFGregorianUnitstoolbox fn CFAbsoluteTimeGetDayOfWeek( CFAbsoluteTime at, CFTimeZoneRef tz ) = SInt32toolbox fn CFAbsoluteTimeGetDayOfYear( CFAbsoluteTime at, CFTimeZoneRef tz ) = SInt32toolbox fn CFAbsoluteTimeGetWeekOfYear( CFAbsoluteTime at, CFTimeZoneRef tz ) = SInt32#endif// ndef _DEFINEDINCARBON