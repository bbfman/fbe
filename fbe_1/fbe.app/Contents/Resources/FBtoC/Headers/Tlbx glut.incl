/*Part of glut.h*/include "Tlbx gl.incl"include library "GLUT" // framework/* GLUT pre-built models sub-API */toolbox glutWireSphere(GLdouble radius, GLint slices, GLint stacks)toolbox glutSolidSphere(GLdouble radius, GLint slices, GLint stacks)toolbox glutWireCone(GLdouble base, GLdouble height, GLint slices, GLint stacks)toolbox glutSolidCone(GLdouble base, GLdouble height, GLint slices, GLint stacks)toolbox glutWireCube(GLdouble size)toolbox glutSolidCube(GLdouble size)toolbox glutWireTorus(GLdouble innerRadius, GLdouble outerRadius, GLint sides, GLint rings)toolbox glutSolidTorus(GLdouble innerRadius, GLdouble outerRadius, GLint sides, GLint rings)toolbox glutWireDodecahedron()toolbox glutSolidDodecahedron()toolbox glutWireTeapot(GLdouble size)toolbox glutSolidTeapot(GLdouble size)toolbox glutWireOctahedron()toolbox glutSolidOctahedron()toolbox glutWireTetrahedron()toolbox glutSolidTetrahedron()toolbox glutWireIcosahedron()toolbox glutSolidIcosahedron()toolbox fn glutExtensionSupported(const char *name) = longtoolbox fn PScstr( Str255 ) = StringPtrlocal fn glutExtensionSupported_P( extension as Str255 )end fn = fn glutExtensionSupported( (^char)fn PScstr( extension ) )