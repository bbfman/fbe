/*   Util_Nib.incl   Bernie Wylde   20090704   Brian 20100906 Changes to use CFStrings  */include "Tlbx IBCarbonRuntime.incl"local fn NibCreateReference( nibName as CFStringRef, outNib as ^IBNibRef )'~'1end fn = fn CreateNibReference( nibName, #outNib )local fn NibCreateWindow( nibName as CFStringRef, wndName as CFStringRef, outWnd as ^WindowRef )'~'1dim as IBNibRef   nibdim as OSStatus   errerr = fn NibCreateReference( nibName, @nib )if ( err ) then exit fnerr = fn CreateWindowFromNib( nib, wndName, #outWnd )DisposeNibReference( nib )end fn = errlocal fn NibCreateMenu( nibName as CFStringRef, menuName as CFStringRef, outMenu as ^MenuRef )'~'1dim as IBNibRef   nibdim as OSStatus   errerr = fn NibCreateReference( nibName, @nib )if ( err ) then exit fnerr = fn CreateMenuFromNib( nib, menuName, #outMenu )DisposeNibReference( nib )end fn = errlocal fn NibSetMenuBar( nibName as CFStringRef, mBarName as CFStringRef )'~'1dim as IBNibRef   nibdim as OSStatus   errerr = fn NibCreateReference( nibName, @nib )if ( err ) then exit fnerr = fn SetMenuBarFromNib( nib, mBarName )DisposeNibReference( nib )end fn = err